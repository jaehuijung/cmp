<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper 
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sl.qr.mh.service.config.configMapper">

    <select id="getSecurityUserInfoList" parameterType="String" resultType="MemberDao">
        select
            id
            password,
            name,
            email,
            phone,
            position,
            group_idx
        from
            user
        where
            id = #{id}
            and is_deleted = 'N'

    </select>

</mapper>